{
  "name": "vaxxed-as-app",
  "description": "Vaxxed As",
  "author": "Rafael Contreras",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "setup": "cd ios && bundle install",
    "pods": "cd ios && bundle exec pod install",
    "lint": "eslint .",
    "format": "prettier --write \"./**/*.{js,ts,css,json,md,mdx,html,yml,yaml,pcss,jsx,tsx}\"",
    "format:locales": "prettier --write \"./locales/**/*.{js,ts,css,json,md,mdx,html,yml,yaml,pcss,jsx,tsx}\"",
    "create-languages": "tsm ./scripts/createLanguages.ts",
    "localazy:download": "export $(xargs <.env) && localazy download -r $LOCALAZY_READ_KEY && yarn format:locales",
    "localazy:keys": "export $(xargs <.env)",
    "localazy:upload": "export $(xargs <.env) && localazy upload -r $LOCALAZY_READ_KEY -w $LOCALAZY_WRITE_KEY",
    "typescript": "tsc --noEmit"
  },
  "dependencies": {
